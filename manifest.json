{
  "manifest_version": 2,

  "name": "Splunk line-expander",
  "description": "Programatically presses the [+] of all lines",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://*/*"]
    }
  ]
}
