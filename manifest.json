{
  "manifest_version": 2,

  "name": "Splunk line-expander",
  "description":
    "Programatically presses the [+] of all message.line items in Splunk",
  "version": "1.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["shared.js"]
  },

  "content_scripts": [
    {
      "js": ["shared.js", "content.js"],
      "matches": ["https://*/*"]
    }
  ]
}
